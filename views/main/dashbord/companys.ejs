<div class="row d-none" id="companies">
    <h3>شركات الشحن المتاحة</h3>
    <ul class="list-group companys">
        <%companys.forEach(c=> { %>
            <% if(c.status=="active" ){ %>
                <li class="list-group-item">
                    <img src="/imgs/saee.jpg" alt="saee">
                    <!-- Button trigger modal -->
                    <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#saeemodal">
                        طلب
                    </button>
                    <div class="modal fade" id="saeemodal" data-bs-backdrop="static" data-bs-keyboard="false"
                        tabindex="-1" aria-labelledby="staticBackdropLabel" aria-hidden="true">
                        <div class="modal-dialog" style="max-width: 60%;">
                            <div class="modal-content">
                                <div class="modal-header">
                                    <h5 class="modal-title" id="staticBackdropLabel">ساعي</h5>
                                    <button type="button" class="btn-close" data-bs-dismiss="modal"
                                        aria-label="Close"></button>
                                </div>
                                <div class="modal-body">
                                    <div class="container-fluid">
                                        <div class="row">
                                            <div class="col-6">
                                                <h3>بينات الشحنة</h3>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">الاسم</label>
                                                    <input type="text" class="form-control" id="p_name">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">الجوال</label>
                                                    <input type="number" class="form-control" id="p_mobile" value="">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">عنوان المستودع</label>
                                                    <input type="text" class="form-control" id="p_streetaddress">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">المدينة</label>
                                                    <input type="text" class="form-control" id="p_city">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">وزن الشحنة بالكجم</label>
                                                    <input type="number" class="form-control" id="weight" value="0">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">الكمية</label>
                                                    <input type="number" class="form-control" id="quantity" value="0">
                                                </div>
                                                <div class="form-check">
                                                    <input class="form-check-input" type="checkbox" value="0"
                                                        id="cashondelivery">
                                                    <label class="form-check-label" for="flexCheckDefault">
                                                        الدفع عند الاستلام
                                                    </label>
                                                </div>
                                            </div>
                                            <div class="col-6">
                                                <h3>بينات المستلم</h3>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">الاسم</label>
                                                    <input type="text" class="form-control" id="c_name">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">الجوال</label>
                                                    <input type="number" class="form-control" id="c_mobile">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">العنوان</label>
                                                    <input type="text" class="form-control" id="c_streetaddress">
                                                </div>
                                                <div class="mb-3">
                                                    <label for="name" class="form-label">المدينة</label>
                                                    <input type="text" class="form-control" id="c_city">
                                                </div>
                                                <div class="mb-3">
                                                    <img src="/imgs/saee.jpg" alt="saee" style="width: 95%;">
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                </div>
                                <div class="modal-footer">
                                    <button type="button" class="btn btn-secondary" onclick="submitOrder()">حفظ</button>
                                </div>
                            </div>
                        </div>
                    </div>
                </li>
                <%}%>
                    <%});%>
    </ul>
</div>
<script src="/dashbord/js/company.js"></script>